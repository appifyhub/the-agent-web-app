  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist .vite node_modules/.vite",
    "build": "tsc -b && vite build && cp dist/index.html dist/404.html",
    "rebuild": "bun run clean && bun run build",
    "dev": "vite",
