{
  "name": "the-agent-web-app",
  "private": true,
  "version": "4.8.1",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist .vite node_modules/.vite",
    "build": "tsc -b && vite build && cp dist/index.html dist/404.html",
    "rebuild": "bun run clean && bun run build",
    "dev": "vite",
    "preview": "vite preview",
    "lint:code": "eslint .",
    "lint:css": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "lint:i18n": "bun run i18n:keys && bun run i18n:check",
    "lint": "bun run lint:i18n && bun run lint:code && bun run lint:css",
    "i18n:keys": "bun scripts/generate-translation-keys.ts",
    "i18n:check": "bun scripts/check-translation-usage.ts"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.554.0",
    "make-plural": "^7.4.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "rollup": "^4.53.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss-custom-media": "^11.0.6",
    "postcss-custom-properties": "^14.0.6",
    "postcss-custom-selectors": "^8.0.5",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "postcss-theme-ui": "^0.10.0",
    "shadcn": "^3.5.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4"
  }
}
