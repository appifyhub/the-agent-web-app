{
  "name": "the-agent-web-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist .vite node_modules/.vite",
    "build": "tsc -b && vite build && cp dist/index.html dist/404.html",
    "rebuild": "bun run clean && bun run build",
    "dev": "vite",
    "preview": "vite preview",
    "lint:code": "eslint .",
    "lint:css": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "lint": "bun run lint:code && bun run lint:css",
    "i18n:keys": "bunx ts-node scripts/generate-translation-keys.ts"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "icons": "^1.0.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.525.0",
    "make-plural": "^7.4.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "rollup": "^4.45.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
